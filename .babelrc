{
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "plugins": ["@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel"],
  "env": {
      "production": {
          "plugins": ["@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel"]
      },
      "development": {
          "plugins": ["@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel"]
      },
      "test": {
          "plugins": ["@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel-test"]
      }
  }
}
